{
  "name": "ddw-leadmaster-system",
  "version": "1.0.0",
  "description": "Sistema unificado de gestión de leads con WhatsApp, curación de datos y respuestas automáticas con IA",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ddw-leadmaster",
    "pm2:restart": "pm2 restart ddw-leadmaster",
    "pm2:logs": "pm2 logs ddw-leadmaster",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "whatsapp",
    "leads",
    "crm",
    "ai",
    "automation",
    "venom-bot",
    "gpt",
    "bulk-sender"
  ],
  "author": "Alberto Hilal",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "venom-bot": "^5.3.0",
    "mysql2": "^3.14.1",
    "openai": "^4.36.0",
    "dotenv": "^16.5.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "winston": "^3.18.3",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "connect-redis": "^9.0.0",
    "redis": "^5.10.0",
    "bcrypt": "^6.0.0",
    "puppeteer": "^24.15.0",
    "puppeteer-core": "^24.32.0",
    "qrcode-terminal": "^0.12.0",
    "dayjs": "^1.11.13",
    "axios": "^1.8.4",
    "geolib": "^3.3.4",
    "papaparse": "^5.5.2",
    "csv-writer": "^1.6.0",
    "json2csv": "^6.0.0-alpha.2",
    "formdata-node": "^6.0.3",
    "pm2": "^6.0.8"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "@playwright/test": "^1.57.0",
    "playwright": "^1.57.0",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "prettier": "^3.2.4",
    "start-server-and-test": "^2.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
